<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="" xml:space="preserve" source-language="en-us" target-language="nl">
    <header>
      <tool tool-id="poeditor.com" tool-name="POEditor"/>
    </header>
    <body>
      <trans-unit id="DonateCharity">
        <source>Charity - US</source>
        <target>Charity - NL</target>
      </trans-unit>
      <trans-unit id="Donate.Continue">
        <source>Continue</source>
        <target>Volgende</target>
      </trans-unit>
      <trans-unit id="Donate.ChooseDifferentAmount">
        <source>Choose a different amount</source>
        <target>Kies een ander bedrag</target>
      </trans-unit>
      <trans-unit id="Donate.ChooseAmount">
        <source>Give as much as you'd like</source>
        <target>Geef zoveel je maar wil</target>
      </trans-unit>
      <trans-unit id="Payment.Continue">
        <source>Give</source>
        <target>Geven</target>
      </trans-unit>
      <trans-unit id="Donate.Give">
        <source>Give</source>
        <target>Geven</target>
      </trans-unit>
      <trans-unit id="Success.ThankYouTitle">
        <source>Thank you for your donation to {organisationName}</source>
        <target>Dankjewel voor je gift aan {organisationName}</target>
      </trans-unit>
      <trans-unit id="Success.WantReceipt">
        <source>I want a receipt</source>
        <target>Ik wil een bonnetje</target>
      </trans-unit>
      <trans-unit id="Success.DownloadApp">
        <source>Download the Givt app</source>
        <target>download de Givt-app</target>
      </trans-unit>
      <trans-unit id="Success.GivtSummary">
        <source>Givt offers you a personal summary and an overview for your tax statement. Managing your tax statement has never been so easy! Want to know more?</source>
        <target>Givt biedt u een persoonlijke samenvatting en een overzicht voor je belastingen. Uw belastingsaangifte doen was nog nooit zo eenvoudig! Meer weten?</target>
      </trans-unit>
      <trans-unit id="Receipt.Title">
        <source>How do you want to receive your receipt?</source>
        <target>Hoe wil je je bonnetje ontvangen?</target>
      </trans-unit>
      <trans-unit id="Receipt.Download">
        <source>download now</source>
        <target>nu downloaden</target>
      </trans-unit>
      <trans-unit id="Receipt.ViaEmail">
        <source>via email</source>
        <target>via e-mail</target>
      </trans-unit>
      <trans-unit id="ReceiptEmail.Title">
        <source>Receive your receipt via email</source>
        <target>Ontvang je bonnetje via e-mail</target>
      </trans-unit>
      <trans-unit id="ReceiptEmail.Send">
        <source>send</source>
        <target>verstuur</target>
      </trans-unit>
      <trans-unit id="ReceiptEmail.SpamText">
        <source>Don’t worry, we won’t send you any spam.</source>
        <target>Geen zorgen, we sturen je geen spam.</target>
      </trans-unit>
      <trans-unit id="PaymentFailure.Title">
        <source>We were unable to complete your donation</source>
        <target>We kunnen je donatie niet voltooien</target>
      </trans-unit>
      <trans-unit id="PaymentFailure.SubTitle">
        <source>If you didn't cancel the donation by yourself, try a different method.</source>
        <target>Als je de donatie niet zelf hebt geannuleerd, probeer dan een andere methode.</target>
      </trans-unit>
      <trans-unit id="PaymentFailure.StartOver">
        <source>Start over</source>
        <target>Probeer opnieuw</target>
      </trans-unit>
      <trans-unit id="Payment.BancontactExplanation">
        <source>After entering your first and last name you will be redirected to the safe environment of Bancontact where you can complete your donation.</source>
        <target>Na het invullen van je voor- en achternaam word je omgeleid naar de veilige omgeving van bancontact om je donatie te vervolledigen.</target>
      </trans-unit>
      <trans-unit id="Payment.BancontactContinue">
        <source>Continue to Bancontact</source>
        <target>Ga naar Bancontact</target>
      </trans-unit>
      <trans-unit id="Payment.IdealContinue">
        <source>Continue to IDeal</source>
        <target>Ga naar IDeal</target>
      </trans-unit>
    </body>
